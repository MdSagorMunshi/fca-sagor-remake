    "path": "latest",
    "pretty-ms": "7.0.1",
    "request": "latest",
    "websocket-stream": "latest",
    "uuid-apikey": "latest",
    "express": "latest",
    "readline": "latest",
    "totp-generator": "latest",
    "synthetic-sagor-database": "latest"
  },
  "engines": {
    "node": ">=14.x <16.x"
  },
  "devDependencies": {
    "eslint": "latest",
    "mocha": "latest",
    "prettier": "latest"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "keywords": [
    "fca sagor",
    "fca-sagor-remake",
    "sagor fca",
    "sagor"
  ]
