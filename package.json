  },
  "dependencies": {
    "aes-js": "latest",
    "assert": "latest",
    "bluebird": "latest",
    "chalk": "4.1.2",
    "cheerio": "latest",
    "crypto": "latest",
    "crypto-js": "latest",
    "got": "^11.8.3",
    "sagor-sp": "latest",
    "https-proxy-agent": "latest",
    "is-hexcolor": "^1.0.0",
    "lodash": "",
    "mqtt": "latest",
    "npmlog": "latest",
